set guioptions-=mTB               " disable toolbars
set showtabline=0                 " disable tab bar

qmark b bbitly                    " bit.ly bookmarklet
qmark j bjisho                    " jisho.org bookmarklet
qmark y bydict                    " dic.yahoo.co.jp bookmarklet
qmark d bdelicious                " delicious.com bookmarklet
qmark s bsupergenpass             " supergenpass bookmarklet

echo                              " silent

" vim:ft=vim
